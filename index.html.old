      <!-- Slide 3: Key Definitions -->
      <section>
        <pre><code>#!/usr/bin/env python3
data = "Data refers to raw facts, figures, and information that can be collected, analyzed, and used to draw conclusions or make decisions."
data_literacy = "This variable refers to the ability to read, interpret, and critically evaluate data."
data_tools = "Technologies used for data management, analysis, archiving and sharing."
data_ontology = "A structured framework that defines relationships and categories for data, allowing for better organization, integration, and understanding of data across different domains."
data_fluency = {
    "data": data,
    "tools": data_tools,
    "data_literacy": data_literacy,
    "data_ontology": data_ontology
}
definition = f"Data fluency involves {data_fluency['data']}, leveraging {data_fluency['tools']}, understanding {data_fluency['data_literacy']}, and utilizing {data_fluency['data_ontology']} to make informed decisions."
print(definition)
exit()
</code></pre>
